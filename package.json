{
  "name": "weekly_programming_problems",
  "version": "0.0.1",
  "description": "Practice with weekly programming problems",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run weeklyproblems",
    "test": "npm run build && npm run mocha",
    "test:debug": "npm run build && npm run mocha:debug",
    "build": "tsc",
    "mocha": "mocha \"build/weekly-problems-tests/**/*.spec.js\"",
    "mocha:debug": "mocha \"build/weekly-problems-tests/**/*.spec.js\" --debug-brk --inspect",
    "weeklyproblems": "node build/weekly-problems/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlein/Weekly-Programming-Problems.git"
  },
  "author": "Tucker Lein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlein/Weekly-Programming-Problems/issues"
  },
  "homepage": "https://github.com/tlein/Weekly-Programming-Problems#readme",
  "dependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/glob": "^5.0.30",
    "@types/inquirer": "0.0.29",
    "@types/mocha": "^2.2.32",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "glob": "https://registry.npmjs.org/glob/-/glob-7.0.5.tgz",
    "inquirer": "^1.2.2",
    "mocha": "^3.1.2",
    "typescript": "^2.0.3"
  }
}
